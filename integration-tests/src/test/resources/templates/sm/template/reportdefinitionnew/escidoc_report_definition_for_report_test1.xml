<?xml version="1.0" encoding="UTF-8"?>
<report-definition 
    xmlns="http://www.escidoc.org/schemas/report-definition/1.0/report-definition"
    xmlns:prop="http://www.escidoc.org/schemas/common/1.0/properties"
    xmlns:srel="http://www.escidoc.org/schemas/common/1.0/structural-relations"
    xmlns:xlink="http://www.w3.org/1999/xlink">
    <properties>
        <prop:name>Report Definition for Report Test</prop:name>
        <srel:scope xlink:href="/statistic/scope/escidoc:scope3"/>
    </properties>
	<sql>
		select 
		page,year,sum(requests) as requests,sum(sessions) as sessions 
		from Report_Test 
		group by page,year order by page, year;
	</sql>
</report-definition>
