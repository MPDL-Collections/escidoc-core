<?xml version="1.0" encoding="UTF-8"?>
<escidocItem:item
	xmlns:relations="http://www.escidoc.org/schemas/common/1.0/relations"
	xmlns:escidocMetadataRecords="http://www.escidoc.org/schemas/common/1.0/md-records"
	xmlns:escidocContentStreams="http://www.escidoc.org/schemas/common/1.0/content-streams"
	xmlns:escidocComponents="http://www.escidoc.org/schemas/item/1.0/components"
	xmlns:version="http://www.escidoc.org/schemas/common/1.0/version"
	xmlns:release="http://www.escidoc.org/schemas/common/1.0/release"
	xmlns:escidocItem="http://www.escidoc.org/schemas/item/1.0/item"
	xmlns:prop="http://www.escidoc.org/schemas/common/1.0/properties"
	xmlns:srel="http://www.escidoc.org/schemas/common/1.0/structural-relations"
	xmlns:xlink="http://www.w3.org/1999/xlink"
	xmlns:xml="http://www.w3.org/XML/1998/namespace"
	xml:base="http://localhost:8080">

	<escidocItem:properties>
		<srel:context xlink:type="simple" xlink:title="A simple Context"
			xlink:href="/ir/context/escidoc:ex1" />
		<srel:content-model xlink:type="simple"
			xlink:title="Generic Content Model"
			xlink:href="/cmm/content-model/escidoc:ex4" />

		<prop:content-model-specific
			xmlns:prop="http://www.escidoc.org/schemas/common/1.0/properties">
			<cms-prop test="1" />
		</prop:content-model-specific>
	</escidocItem:properties>

	<escidocMetadataRecords:md-records>
		<escidocMetadataRecords:md-record name="escidoc">
			<metadata />
		</escidocMetadataRecords:md-record>

	</escidocMetadataRecords:md-records>


	<escidocComponents:components>

		<escidocComponents:component>
			<escidocComponents:properties>
				<prop:valid-status>valid</prop:valid-status>
				<prop:visibility>public</prop:visibility>
				<prop:content-category>pre-print</prop:content-category>
				<prop:file-name>eSciDoc Banner 2008</prop:file-name>
				<prop:mime-type>image/jpeg</prop:mime-type>
			</escidocComponents:properties>

			<escidocComponents:content
				xlink:href="http://localhost:8080/escidoc/images/escidoc-banner.jpg"
				storage="internal-managed" />
		</escidocComponents:component>

	</escidocComponents:components>

</escidocItem:item>

