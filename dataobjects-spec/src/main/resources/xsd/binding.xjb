        <schemaBindings>
            <package name="org.escidoc.core.domain.context"/>
        </schemaBindings>
        <bindings node="//xs:element[@name='properties']">
            <class name="ContextPropertiesTO"/>
        </bindings>
        <bindings node="//xs:element[@name='resources']">
            <class name="ContextResourcesTO"/>
        </bindings>
        <bindings node="//xs:element[@name='admin-descriptor']">
            <class name="AdminDescriptorTO"
                   implClass="org.escidoc.core.domain.context.AdminDescriptorDatastreamHolderTO"/>
        </bindings>
        <bindings node="//xs:element[@name='admin-descriptors']">
            <class name="AdminDescriptorsTO"/>
        </bindings>
        <bindings node="//xs:element[@name='context']">
            <class name="ContextTO"/>
        </bindings>
    </bindings>
    <bindings schemaLocation="item-list.xsd">
        <schemaBindings>
            <package name="org.escidoc.core.domain.item"/>
        </schemaBindings>
        <bindings node="//xs:element[@name='item-list']">
            <class name="ItemListTO"/>
        </bindings>
    </bindings>
    <bindings schemaLocation="item.xsd">
        <schemaBindings>
            <package name="org.escidoc.core.domain.item"/>
        </schemaBindings>
        <bindings node="//xs:element[@name='item']">
            <class name="ItemTO"/>
        </bindings>
        <bindings node="//xs:element[@name='properties']">
            <class name="ItemPropertiesTO"/>
        </bindings>
    </bindings>
    <bindings schemaLocation="md-records.xsd">
        <schemaBindings>
            <package name="org.escidoc.core.domain.metadatarecords"/>
        </schemaBindings>
        <bindings node="//xs:element[@name='md-record']">
            <class name="MdRecordTO" implClass="org.escidoc.core.domain.metadatarecords.MdRecordDatastreamHolderTO"/>
        </bindings>
        <bindings node="//xs:element[@name='md-records']">
            <class name="MdRecordsTO"/>
        </bindings>
    </bindings>
    <bindings schemaLocation="organizational-unit.xsd">
        <schemaBindings>
            <package name="org.escidoc.core.domain.ou"/>
        </schemaBindings>
        <bindings node="//xs:element[@name='properties']">
            <class name="OrganizationalUnitPropertiesTO"/>
        </bindings>
        <bindings node="//xs:element[@name='organizational-unit']">
            <class name="OrganizationalUnitTO"/>
        </bindings>
        <bindings node="//xs:element[@name='parents']">
            <class name="ParentsTO"/>
        </bindings>
        <bindings node="//xs:element[@name='predecessors']">
            <class name="PredecessorsTO"/>
        </bindings>
    </bindings>
    <bindings schemaLocation="organizational-unit-list.xsd">
        <schemaBindings>
            <package name="org.escidoc.core.domain.ou"/>
        </schemaBindings>
        <bindings node="//xs:element[@name='organizational-unit-list']">
            <class name="OrganizationalUnitListTO"/>
        </bindings>
    </bindings>
    <bindings schemaLocation="properties.xsd">
        <schemaBindings>
            <package name="org.escidoc.core.domain.properties"/>
        </schemaBindings>
        <bindings node="//xs:element[@name='affiliations']">
            <class name="AffiliationsTO"/>
        </bindings>
        <bindings node="//xs:element[@name='content-model-specific']">
            <class name="ContentModelSpecificTO"
                   implClass="org.escidoc.core.domain.properties.ContentModelSpecificDatastreamHolderTO"/>
        </bindings>
        <bindings node="//xs:element[@name='external-ids']">
            <class name="ExternalIdsTO"/>
        </bindings>
        <bindings node="//xs:element[@name='latest-release']">
            <class name="LatestReleaseTO"/>
        </bindings>
        <bindings node="//xs:element[@name='latest-version']">
            <class name="LatestVersionTO"/>
        </bindings>
        <bindings node="//xs:element[@name='organizational-units']">
            <class name="OrganizationalUnitsTO"/>
        </bindings>
        <bindings node="//xs:element[@name='version']">
            <class name="VersionTO"/>
        </bindings>
        <bindings node="//xs:element[@name='type']">
            <property name="resourceType"/>
        </bindings>
    </bindings>
    <bindings schemaLocation="relations.xsd">
        <schemaBindings>
            <package name="org.escidoc.core.domain.relations"/>
        </schemaBindings>
        <bindings node="//xs:element[@name='relations']">
            <class name="RelationsTO"/>
        </bindings>
    </bindings>
    <bindings schemaLocation="structural-relations.xsd">
        <schemaBindings>
            <package name="org.escidoc.core.domain.relations"/>
        </schemaBindings>
        <bindings node="//xs:element[@name='affiliation']">
            <class name="AffiliationTO"/>
        </bindings>
        <bindings node="//xs:element[@name='granted-to']">
            <class name="GrantedToTO"/>
        </bindings>
        <bindings node="//xs:complexType[@name='predecessorLinkRequired']">
            <class name="PredecessorLinkRequiredTO"/>
        </bindings>
    </bindings>
    <bindings schemaLocation="struct-map.xsd">
        <schemaBindings>
            <package name="org.escidoc.core.domain.relations"/>
        </schemaBindings>
        <bindings node="//xs:element[@name='struct-map']">
            <class name="StructMapTO"/>
        </bindings>
    </bindings>
    <bindings schemaLocation="release.xsd">
        <schemaBindings>
            <package name="org.escidoc.core.domain.release"/>
        </schemaBindings>
    </bindings>
    <bindings schemaLocation="version.xsd">
        <schemaBindings>
            <package name="org.escidoc.core.domain.version"/>
        </schemaBindings>
    </bindings>
    <bindings schemaLocation="version-history.xsd">
        <schemaBindings>
            <package name="org.escidoc.core.domain.version"/>
        </schemaBindings>
        <bindings node="//xs:element[@name='version-history']">
            <class name="VersionHistoryTO"/>
        </bindings>
        <bindings node="//xs:element[@name='version']">
            <class name="VersionTO"/>
        </bindings>
        <bindings node="//xs:element[@name='timestamp']">
            <property name="lastEventTimestamp"/>
        </bindings>
        <bindings node="//xs:element[@name='events']">
            <class name="EventsTO"/>
        </bindings>
    </bindings>
    <bindings schemaLocation="Event-v1-1.xsd">
        <schemaBindings>
            <package name="org.escidoc.core.domain.version"/>
        </schemaBindings>
        <bindings node="//xs:element[@name='event']">
            <class name="EventTO"/>
        </bindings>
    </bindings>
</bindings>