<?xml version="1.0" encoding="UTF-8"?>
<bindings xmlns="http://java.sun.com/xml/ns/jaxb" xmlns:xsi="http://www.w3.org/2000/10/XMLSchema-instance"
          xmlns:xs="http://www.w3.org/2001/XMLSchema"
          xsi:schemaLocation="http://java.sun.com/xml/ns/jaxb http://java.sun.com/xml/ns/jaxb/bindingschema_2_0.xsd"
          version="2.1">
    <bindings>
        <globalBindings>
            <javaType name="org.joda.time.DateTime" xmlType="xs:date"
                      parseMethod="org.esidoc.core.utils.xml.DateTimeJaxbConverter.parseDate"
                      printMethod="org.esidoc.core.utils.xml.DateTimeJaxbConverter.printDate"/>
            <javaType name="org.joda.time.DateTime" xmlType="xs:dateTime"
                      parseMethod="org.esidoc.core.utils.xml.DateTimeJaxbConverter.parseDate"
                      printMethod="org.esidoc.core.utils.xml.DateTimeJaxbConverter.printDate"/>
            <serializable/>
        </globalBindings>
    </bindings>
    <bindings schemaLocation="common-types.xsd">
        <schemaBindings>
            <package name="org.escidoc.core.domain.common"/>
        </schemaBindings>
        <bindings node="//xs:complexType[@name='laxLinkContainer']">
            <class name="LaxLinkContainerTO"/>
        </bindings>
        <bindings node="//xs:complexType[@name='link']">
            <class name="LinkTO"/>
        </bindings>
        <bindings node="//xs:complexType[@name='linkForCreate']">
            <class name="LinkForCreateTO"/>
        </bindings>
        <bindings node="//xs:complexType[@name='linkRequired']">
            <class name="LinkRequiredTO"/>
        </bindings>
        <bindings node="//xs:complexType[@name='linkWithoutObjid']">
            <class name="LinkWithoutObjidTO"/>
        </bindings>
        <bindings node="//xs:complexType[@name='readOnlyLink']">
            <class name="ReadOnlyLinkTO"/>
        </bindings>
    </bindings>
    <bindings schemaLocation="components.xsd">
        <schemaBindings>
            <package name="org.escidoc.core.domain.componets"/>
        </schemaBindings>
        <bindings node="//xs:element[@name='component']">
            <class name="ComponentTO"/>
        </bindings>
        <bindings node="//xs:element[@name='components']">
            <class name="ComponentsTO"/>
        </bindings>
        <bindings node="//xs:element[@name='properties']">
            <class name="ComponentPropertiesTO"/>
        </bindings>
    </bindings>
    <bindings schemaLocation="container-list.xsd">
        <schemaBindings>
            <package name="org.escidoc.core.domain.container"/>
        </schemaBindings>
        <bindings node="//xs:element[@name='container-list']">
            <class name="ContainerListTO"/>
        </bindings>
    </bindings>
    <bindings schemaLocation="container.xsd">
        <schemaBindings>
            <package name="org.escidoc.core.domain.container"/>
        </schemaBindings>
        <bindings node="//xs:element[@name='container']">
            <class name="ContainerTO"/>
        </bindings>
        <bindings node="//xs:element[@name='properties']">
            <class name="ContainerPropertiesTO"/>
        </bindings>
    </bindings>
    <bindings schemaLocation="content-model.xsd">
        <schemaBindings>
            <package name="org.escidoc.core.domain.content.model"/>
        </schemaBindings>
        <bindings node="//xs:element[@name='content-model']">
            <class name="ContentModelTO"/>
        </bindings>
        <bindings node="//xs:element[@name='properties']">
            <class name="ContentModelPropertiesTO"/>
        </bindings>
    </bindings>
    <bindings schemaLocation="content-relation.xsd">
        <schemaBindings>
            <package name="org.escidoc.core.domain.content.relation"/>
        </schemaBindings>
        <bindings node="//xs:element[@name='content-relation']">
            <class name="ContentRelationTO"/>
        </bindings>
        <bindings node="//xs:element[@name='type']">
            <class name="ContentRelationTypeTO"/>
            <property name="contentRelationType"/>
        </bindings>
        <bindings node="//xs:element[@name='properties']">
            <class name="ContentRelationPropertiesTO"/>
        </bindings>
        <bindings node="//xs:element[@name='resources']">
            <class name="ContentRelationResourcesTO"/>
        </bindings>
        <bindings node="//xs:element[@name='object']">
            <class name="ContentRelationObjectTO"/>
        </bindings>
        <bindings node="//xs:element[@name='subject']">
            <class name="SubjectTO"/>
        </bindings>
    </bindings>
    <bindings schemaLocation="content-streams.xsd">
        <schemaBindings>
            <package name="org.escidoc.core.domain.content.stream"/>
        </schemaBindings>
        <bindings node="//xs:element[@name='content-stream']">
            <class name="ContentStreamTO"
                   implClass="org.escidoc.core.domain.content.stream.ContentStreamDatastreamHolderTO"/>
        </bindings>
        <bindings node="//xs:element[@name='content-streams']">
            <class name="ContentStreamsTO"/>
        </bindings>
    </bindings>
    <bindings schemaLocation="context-list.xsd">
        <schemaBindings>
            <package name="org.escidoc.core.domain.context"/>
        </schemaBindings>
        <bindings node="//xs:element[@name='context-list']">
            <class name="ContextListTO"/>
        </bindings>
    </bindings>
