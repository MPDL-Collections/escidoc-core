<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema"
           xmlns:organizational-unit="http://www.escidoc.de/schemas/organizationalunit/0.9"
           xmlns:common="http://www.escidoc.de/schemas/commontypes/0.4"
           xmlns:organizational-unit-list="http://www.escidoc.de/schemas/organizationalunitlist/0.9"
           targetNamespace="http://www.escidoc.de/schemas/organizationalunitlist/0.9"
           elementFormDefault="qualified" attributeFormDefault="unqualified">

  <xs:import namespace="http://www.w3.org/XML/1998/namespace" schemaLocation="xml.xsd"/>
  <xs:import namespace="http://www.escidoc.de/schemas/commontypes/0.4" schemaLocation="common-types.xsd"/>
  <xs:import namespace="http://www.escidoc.de/schemas/organizationalunit/0.9"
             schemaLocation="organizational-unit.xsd"/>

  <xs:element name="organizational-unit-list" type="organizational-unit-list:organizationalUnitListType"/>

  <xs:complexType name="organizationalUnitListType">
    <xs:sequence minOccurs="0" maxOccurs="unbounded">
      <xs:element ref="organizational-unit:organizational-unit"/>
    </xs:sequence>
    <xs:attributeGroup ref="common:listAttributes"/>
  </xs:complexType>
</xs:schema>
